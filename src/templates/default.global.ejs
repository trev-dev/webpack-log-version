{
	name: '<%= name %>',
	version: '<%= version %>',
	date: '<%= date %>',
	<% if (git) { %>
	branch: '<%= branch %>',
	<% if (dirty) { %>
	dirty: true,
	<% } %>
	commit: '<%= commit %>',
	<% } %>
	<% if (deployedBy) { %>
	deployedBy: '<%= deployedBy %>',
	<% } %>
	<% if (comment !== '') { %>
	comment: '<%= comment %>'
	<% } %> 
}